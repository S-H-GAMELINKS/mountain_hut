<div class="container">
  <% @page_title = "職員情報の編集" %>

  <h1><%= @page_title %></h1>

  <div class="toolbar">
    <%= link_to "職員の詳細に戻る", @member %>
  </div>

  <%# パラメータ名をaccount、送信先urlを/accountに変更している %>
  <%= form_for @member, as: "account", url: :account do |form| %>
    <%= render "shared/member_form", form: form %>
    <div><%= form.submit %></div>
  <% end %>
</div>
